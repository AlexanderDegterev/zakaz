CREATE TABLE T_SERV_M (
    SVM_ID         INTEGER NOT NULL,
    SVM_UID        INTEGER,
    SVM_TPID       VARCHAR(10),
    SVM_COUNT      NUMERIC(15,4),
    SVM_READY      NUMERIC(15,4),
    SVM_TP_VOLUME  NUMERIC(15,2),
    SVM_UNITM      VARCHAR(15),
    SVM_SUMM       NUMERIC(15,2)
);


ALTER TABLE T_SERV_M ADD CONSTRAINT PK_T_SERV_M PRIMARY KEY (SVM_ID);
CREATE TABLE T_object (
    Ob_ID         INTEGER NOT NULL,
    Ob_NAME       VARCHAR(200),
    Ob_ADRES      VARCHAR(200),
    Ob_STATUS     INTEGER,
    Ob_COUNTRY    VARCHAR(3),
    Ob_NOMERACC   VARCHAR(20),
    Ob_BANKID     VARCHAR(15),
    Ob_UNN        VARCHAR(15),
    Ob_OKPO       VARCHAR(20),
    Ob_INN        VARCHAR(20),
    Ob_PHID       INTEGER,
    Ob_PHONE      VARCHAR(75),
    Ob_POSTADRES  VARCHAR(200),
    Ob_EMAIL      VARCHAR(100),
    Ob_BANKNAME   VARCHAR(100),
    Ob_PASSNOMER  VARCHAR(20),
    Ob_PASSDATE   DATE,
    Ob_PASSPLACE  VARCHAR(50),
    Ob_BANKADDR   VARCHAR(70),
    Ob_REGADRES   VARCHAR(70)
);


ALTER TABLE T_object ADD CONSTRAINT PK_T_object PRIMARY KEY (Ob_ID);
